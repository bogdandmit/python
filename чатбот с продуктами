print("Для начала введите start()")
def start():
    x=["помидоры","морковь","картошка","свёкла"]
    print(x, " Это ваш список продуктов")
    print(" Для получения списка команд введите help")
    while z!="exit":
        z=input(" Введите команду ")
        if z == "help":
            print(" Delite для удаления из списка\n",
                    "Isert для добавления в список\n",
                    "Print для показа элемента списка\n",
                    "Print all для показа всего списка")
        if z== "delite":
            c=int(input("Введите индекс продукта"))
            del x[c]
        if z == "insert":
            v=int(input("Введите индекс продукта"))
            x.insert(v, input("Введите имя продукта"))
        z=input(" Введите команду ")
        if z == "print":
            b=int(input("Введите индекс продукта который хотите увидеть"))
            print(x[b])
        z=input(" Введите команду ")
        if z == "print all":
            print(x)
